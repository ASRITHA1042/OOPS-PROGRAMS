#include<iostream>
using namespace std;
int main()
{
  int arr[50],x,max,min,i;
  cout<<"array size"<<x;
  cin>>x;
  cout<<"Enter elements\n ";
  for(i=0;i<x;i++)
  cin>>arr[i];
  cout<<"array: ";
  for(i=0;i<x;i++)
  cout<<arr[i]<<"  ";
  max=arr[0];
  min=arr[0];
  for(i=0;i<x;i++)
  if(max<arr[i])
   max=arr[i];
   else if(min>arr[i])
      min=arr[i];
    cout<<"\nMax element in array: "<<max;
    cout<<"\nMin element in Array: "<<min;
  return 0;
}
