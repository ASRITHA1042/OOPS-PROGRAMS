#include<iostream>
using namespace std;
int main()
{
	char val;
	int a=20;
	int b=5;
	int c;
	cout<<"a.addition";
	cout<<"b.substraction";
	cout<<"c.multiplication";
	cin>>val;
	switch(val)
	{
		case'a':
			c=a+b;
			cout<<c;
			break;
		case'b':
			c=a-b;
			cout<<c;
			break;
		case 'c':
		    c=a*b;
			cout<<c;	
		default:
			cout<<"provide a or b";
	}
	return 0;
}
	
