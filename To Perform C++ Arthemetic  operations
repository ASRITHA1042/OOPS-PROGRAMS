#include<iostream>
int main()
{
    int a,b,c,d,e,f,g;
    a=21;
    b=2;
    c=a+b;
    {
    std::cout<<"\nc=" <<c;
}
     d=a-b;
     {
    std::cout<<"\nd=" <<d;
}
    e=a*b;
    {
    std::cout<<"\ne=" <<e;
}
     f=a/b;
     {
    std::cout<<"\nf=" <<f;
}
    g=a%b;
    {
   std::cout<<"\ng=" <<g;
}
    return 0;
}
