#include<iostream>
using namespace std;
class base
{
	public:
		void a()
		{
			cout<<"\nfirst";
		}
};
class derived1:public base
{
	public:
		void b()
		{
			cout<<"\nsecond";
		}
};
class derived2:public base
{
	public:
		void c()
		{
			cout<<"\nthird";
		}
};
class derived3:public base
{
	public:
		void d()
		{
			cout<<"\nfourth";
		}
};
int main()
{
	derived1 obj1;
	derived2 obj2;
	derived3 obj3;
	obj1.a();
	obj1.b();
	obj2.a();
	obj2.c();
	obj3.a();
	obj3.d();
	return 0;
}
