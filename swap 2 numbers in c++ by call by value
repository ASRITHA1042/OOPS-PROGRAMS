#include<iostream>
using namespace std;
int main()
{
	int x,y;
	cout<<"enter x=";
	cin>>x;
	cout<<"enter y=";
	cin>>y;
	cout<<"\nbefore swapping"<<endl;
	cout<<"x="<<x<<"\ny="<<y<<endl;
	int swap;
	swap=x;
	x=y;
	y=swap;
    cout<<"\nafter swapping:"<<endl;
    cout<<"x="<<x<<"\ny="<<y<<endl;
    return 0;
}
